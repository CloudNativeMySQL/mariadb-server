--echo #

SELECT t1.a,t1.c,MAX(t1.b)
FROM t1
WHERE t1.a=t1.c
GROUP BY t1.a
HAVING t1.c > 1;

SELECT t1.a,t1.c,MAX(t1.b)
FROM t1
WHERE t1.a=t1.c+1
GROUP BY t1.a
HAVING t1.c > 1;

SELECT t1.a,t1.c,t1.d
FROM t1
WHERE t1.a=t1.c AND t1.a=t1.d
GROUP BY t1.a
HAVING t1.c > 1 AND t1.d < 3;

SELECT t1.a,t1.d
FROM t1
WHERE t1.a=t1.d
GROUP BY t1.a
HAVING MAX(t1.c) > 1 OR t1.d < 3;

SELECT t1.a,t1.c
FROM t1
WHERE t1.a=t1.c
GROUP BY t1.a
HAVING 1;

