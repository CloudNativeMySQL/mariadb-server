SET SQL_MODE='only_full_group_by';

CREATE TABLE t1(a INT, b INT, c INT, d INT);
CREATE TABLE t2 (x INT, y BLOB, z BLOB, w BLOB);

INSERT INTO t1 VALUES (1,2,1,1),(1,3,0,4),(2,4,2,2);
INSERT INTO t2 VALUES (2,'aab','aab','x'),(1,'aa','b','.'),
                      (1,'ddd','n ','s'), (1,'ddd','ddd ','d');

--echo #
--echo #
--echo # SELECT list checking
--echo #
--echo #

SELECT 1 GROUP BY 1;

SELECT 1 FROM t1 GROUP by t1.a;

SELECT t1.a,t1.c,MAX(t1.b)
FROM t1
WHERE t1.a=t1.c
GROUP BY t1.a;

SELECT t1.a,t1.c,t1.d,MAX(t1.b)
FROM t1
WHERE t1.a=t1.c AND t1.a=t1.d
GROUP BY t1.a;

SELECT t1.a,t1.c,t1.d,MAX(t1.b)
FROM t1
WHERE t1.a=t1.c AND t1.c=t1.d
GROUP BY t1.a;

SELECT *
FROM t1
GROUP BY t1.a,t1.b,t1.c,t1.d;

SELECT t1.a REGEXP '.*'
FROM t1
GROUP BY t1.a;

SELECT t1.a&t1.b
FROM t1
GROUP BY t1.a,t1.b;

SELECT t1.a|t1.b,~t1.c
FROM t1
GROUP BY t1.a,t1.b,t1.c;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t1.a,t1.c,MAX(t1.b)
FROM t1
GROUP BY t1.a;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t1.a+t1.c,MAX(t1.b)
FROM t1
GROUP BY t1.a;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t1.a
FROM t1
GROUP BY t1.a+1;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t1.a,t1.c,t1.d,MAX(t1.b)
FROM t1
WHERE t1.a=1 AND t1.c=t1.d
GROUP BY t1.a;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT *
FROM t1
GROUP BY t1.a;

--echo #
--echo # NUMERIC functions
--echo #

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t1.a,t1.b
FROM t1
WHERE t1.a+2=t1.b+1/2
GROUP BY t1.b;

SELECT t1.a,t1.b
FROM t1
WHERE t1.b+t1.c+1/2=t1.a+1/2
GROUP BY t1.b,t1.c;

SELECT t1.a,t1.b
FROM t1
WHERE t1.b/t1.c=t1.a+1/2
GROUP BY t1.b,t1.c;

SELECT t1.a,t1.c
FROM t1
WHERE t1.a % t1.b=t1.c
GROUP BY t1.a,t1.b;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t1.a,t1.b
FROM t1
WHERE t1.b+t1.c=t1.a+1/2
GROUP BY t1.b,t1.c;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t1.a,t1.b
FROM t1
WHERE t1.b+2=t1.a+1/2
GROUP BY t1.b;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t1.a,t1.b
FROM t1
WHERE t1.b % t1.c=t1.a+1/2
GROUP BY t1.b,t1.c;

--echo # BETWEEN

SELECT t1.a BETWEEN 1 AND 3 AS bw_col,t1.a
FROM t1
GROUP BY t1.a;

SELECT t1.b BETWEEN t1.c AND 3 AS bw_col,t1.b,t1.c
FROM t1
GROUP BY t1.b,t1.c;

SELECT t1.a,MAX(t2.y)
FROM t1,t2
WHERE t1.a=t2.x
GROUP BY t2.x;

SELECT t1.a,MAX(t1.b)
FROM t1,t2
WHERE t1.a=LENGTH(t2.y)
GROUP BY t2.y;

SELECT t1.a,MAX(t1.b)
FROM t1,t2
WHERE t1.a=LENGTH(t2.y)+1
GROUP BY t2.y;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t1.a,MAX(t1.b)
FROM t1,t2
WHERE t1.a=(LENGTH(t2.y)+1)/2
GROUP BY t2.y;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t1.a,MAX(t1.b)
FROM t1,t2
WHERE t1.a/2=(LENGTH(t2.y)+1)
GROUP BY t2.y;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t1.a,MAX(t1.b)
FROM t1,t2
WHERE t1.a=LENGTH(t2.y)
GROUP BY t2.x;

--echo # Function in GROUP BY

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t1.a
FROM t1
GROUP BY t1.a/3;

--echo # PK-key check

CREATE TABLE t3(pk INT PRIMARY KEY, b INT, c INT, d INT);
INSERT INTO t3 VALUES (1,2,1,1),(2,3,3,4),(3,4,2,2);

SELECT t3.pk,t3.c,MAX(t3.b)
FROM t3
GROUP BY t3.pk;

SELECT t3.pk,t3.c,MAX(t3.b)
FROM t3
WHERE t3.c=t3.d
GROUP BY t3.pk;

SELECT t3.pk,t3.c,MAX(t3.b)
FROM t3
WHERE t3.pk=t3.d
GROUP BY t3.d;

SELECT t3.pk,t1.a,MAX(t3.b)
FROM t3,t1
WHERE t3.c=t1.a
GROUP BY t3.pk;

SELECT t3.pk,t3.b,t1.a,MAX(t3.b)
FROM t3,t1
WHERE t3.pk=t1.a
GROUP BY t1.a;

SELECT t1.a,t3.pk,MAX(t1.b)
FROM t1,t2,t3
WHERE t1.a=LENGTH(t2.y) AND t1.a=t3.pk
GROUP BY t3.pk;

SELECT t1.a,t3.pk,MAX(t1.b)
FROM t1,t2,t3
WHERE t1.a=LENGTH(t2.y) AND t1.a=t3.b
GROUP BY t3.pk;

SELECT *
FROM t3
GROUP BY t3.pk;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t1.a,t3.pk,MAX(t1.b)
FROM t1,t2,t3
WHERE t1.a=LENGTH(t2.y) AND t1.a=t3.pk
GROUP BY t3.b;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t3.pk,t1.a,MAX(t3.b)
FROM t3,t1
WHERE t3.c=t1.b
GROUP BY t3.pk;

DROP TABLE t3;

--echo # Unique keys

CREATE TABLE t3 (a INT, b INT, c CHAR(5), d INT, e INT,
                 UNIQUE idx1 (a,b), UNIQUE idx2 (c));
INSERT INTO t3 VALUES (1,2,'ccc',4,1), (2,2,'aaa',4,3), (2,3,'ex',1,5);

SELECT t3.a,t3.b,t3.d
FROM t3
GROUP BY t3.a,t3.b;

SELECT t3.a,t3.b,t3.d
FROM t3
GROUP BY t3.c;

SELECT *
FROM t3
GROUP BY t3.c;

SELECT *
FROM t3
GROUP BY t3.a,t3.b;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t3.a,t3.b
FROM t3
GROUP BY t3.a;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t3.a,t3.b,t3.d
FROM t3
WHERE t3.a=t3.d
GROUP BY t3.a;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT *
FROM t3
GROUP BY t3.a;

DROP TABLE t3;

--echo # Virtual column

CREATE TABLE t3 (a INT, b INT, c INT AS (a*b) VIRTUAL,
                 d INT AS (b*b*2) VIRTUAL, e INT AS (1*2));
INSERT INTO t3 VALUES (1,3,default,default,default),
                      (2,2,default,default,default),
                      (1,2,default,default,default);

SELECT t3.a,t3.c
FROM t3
GROUP BY t3.a,t3.c;

SELECT t3.a,t3.c
FROM t3
GROUP BY t3.a,t3.b;

SELECT t3.a,t3.c,t3.d
FROM t3
GROUP BY t3.a,t3.b;

SELECT t3.d
FROM t3
GROUP BY t3.b;

SELECT t3.e
FROM t3
GROUP BY t3.e;

SELECT t3.e
FROM t3
GROUP BY t3.e+1;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t3.c
FROM t3
GROUP BY t3.b;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t3.d
FROM t3
GROUP BY t3.d+1;

DROP TABLE t3;

--echo # IN func

SELECT t2.x IN (3, 1) as in_col FROM t2 GROUP BY t2.x;

SELECT t2.y IN ('aa', t2.z) as in_col
FROM t2 GROUP BY t2.y,t2.z;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t2.y IN ('aa', t2.z, t2.w) as in_col
FROM t2 WHERE t2.z=t2.w+1 GROUP BY t2.y,t2.z;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t2.x IN (3, t2.y) as in_col
FROM t2 GROUP BY t2.x,t2.y;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t2.x IN ('aa', 1) as in_col FROM t2 GROUP BY t2.x;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
SELECT t2.y
FROM t2
WHERE 1 IN (2,3) = t2.y
GROUP BY t2.x;

--error ER_NO_FUNCTIONAL_DEPENDENCE_ON_GROUP_BY
